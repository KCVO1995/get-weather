#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir//usr/bin/env" ]; then
  "$basedir//usr/bin/env"  "$basedir/../get-weather3/index.js" "$@"
  ret=$?
else 
  /usr/bin/env  "$basedir/../get-weather3/index.js" "$@"
  ret=$?
fi
exit $ret
